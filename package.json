    "@fastify/static": "^8.0.3",
    "@fastify/type-provider-typebox": "^5.0.1",
    "@fastify/view": "^10.0.1",
    "@prisma/client": "^6.2.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "config": "^3.3.12",
    "fastify": "^5.1.0",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "materialize-css": "^1.0.0-rc.2",
    "nanoid": "^3.3.8",
    "sass": "^1.81.0",
    "svelte": "^5.1.16",
    "tsc": "^2.0.4",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
