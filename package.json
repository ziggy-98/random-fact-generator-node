{
  "scripts": {
    "start": "node ./dist/index.js",
    "build:app": "babel --extensions '.ts' src -d dist --copy-files",
    "build:sass": "sass src/client/sass/app.scss src/client/css/app.css",
    "build": "npm run build:sass && npm run build:app",
    "watch": "babel --watch --extensions '.ts' src -d dist -s --copy-files",
    "start:local": "nodemon ./dist/index.js",
    "dev": "concurrently 'npm run watch' 'npm run start:local'"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.1",
