{
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "babel --extensions '.ts' src -d dist --copy-files",
    "watch": "babel --watch --extensions '.ts' src -d dist --copy-files",
    "start:local": "nodemon ./dist/index.js",
    "dev": "concurrently 'npm run watch' 'npm run start:local'"
  },
  "dependencies": {
    "@fastify/type-provider-typebox": "^5.0.1",
