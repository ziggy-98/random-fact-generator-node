    "@types/config": "^3.3.5",
    "@types/jsonwebtoken": "^9.0.7",
    "babel-cli": "^6.26.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.2.6",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "prisma": "^6.2.1",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.13.0"
  },
